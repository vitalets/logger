{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "noEmit": true,
    "strict": true,
    "useUnknownInCatchVariables": false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": [ "src", "test", "scripts" ]
}
